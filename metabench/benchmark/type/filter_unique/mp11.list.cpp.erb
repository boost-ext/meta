#include <boost/mp11.hpp>

template<class...> struct type_list { };

template<class T>
using has_value = boost::mp11::mp_bool<requires(T t) { t.value; }>;

template<class... Ts>
using filter_unique = boost::mp11::mp_unique<boost::mp11::mp_filter<has_value, type_list<Ts...>>>;

template<int> struct x;

<% env[:k].times do |k| %>

  <% (env[:p]*n/100).times do |p| %>
  #if defined(METABENCH)
  using <%= "result#{p}#{k}" %> = filter_unique<<%= ((1)..(n)).map { |i| "x<#{i}>" }.join(', ') %>, <%= ((1)..(n)).map { |i| "x<#{i}>" }.join(', ') %>>;
  #endif
  <% end %>

<% end %>

int main() {}
